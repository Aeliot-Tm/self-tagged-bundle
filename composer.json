{
  "name": "aeliot-tm/self-tagged-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "This bundle provides tool for updating of services definitions tagging.",
  "keywords": [
    "symfony",
    "dependency injection"
  ],
  "autoload": {
    "psr-4": {
      "Aeliot\\SelfTaggedBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aeliot\\SelfTaggedBundle\\Test\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/http-kernel": "^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/phpstan-strict-rules": "^1.4",
    "phpstan/phpstan-symfony": "^1.2",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "symfony/console": "^6.0",
    "symfony/debug-bundle": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/framework-bundle": "^6.0",
    "symfony/yaml": "^6.2"
  }
}
